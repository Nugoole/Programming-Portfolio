<Window x:Class="ALT.BoltHeight.Views.CameraViewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ALT.BoltHeight.Views"
        mc:Ignorable="d"
        Title="CameraViewWindow" Height="950" Width="980" Background="White" DataContext="{Binding Camera, Source={StaticResource Locator}}" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="391*"/>
            <ColumnDefinition Width="95*"/>
        </Grid.ColumnDefinitions>
        <local:UCCameraView Margin="0,0,0,0" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
